<launch>
  <include
    file="$(find gazebo_ros)/launch/empty_world.launch" />
  <node
    name="tf_footprint_bot_Base"
    pkg="tf"
    type="static_transform_publisher"
    args="0 0 0 0 0 0 bot_Base_link bot_Base_footprint 40" />
  <node
    name="spawn_model"
    pkg="gazebo_ros"
    type="spawn_model"
    args="-file $(find zbot)/urdf/bot.urdf -urdf -model bot"
    output="screen" />
  <node
    name="fake_joint_calibration"
    pkg="rostopic"
    type="rostopic"
    args="pub /calibrated std_msgs/Bool true" />
  <node pkg="zbot" type="bot.py" name="bot_cmd_vel" output="screen">
  </node >
</launch>